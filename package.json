{
  "name": "@platojar/system-two",
  "version": "0.0.6",
  "description": "",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "start": "npm-run-all --print-label -p dev:*",
    "serve": "npm-run-all --print-label -p serve:*",
    "clean": "npm-run-all --print-label -p clean:*",
    "build": "npm-run-all --print-label -p clean:* -p build:*",
    "pack": "npm run pack --prefix packages/server",
    "release": "node ./scripts/release.js",
    "clean:editor": "npm run clean --prefix packages/editor",
    "clean:server": "npm run clean --prefix packages/server",
    "dev:editor": "npm run dev --prefix packages/editor",
    "dev:server": "npm run dev --prefix packages/server",
    "build:editor": "npm run build --prefix packages/editor",
    "build:server": "npm run build --prefix packages/server",
    "serve:server": "npm run serve --prefix packages/server"
  },
  "workspaces": [
    "packages/*"
  ]
}
